<!doctype html>
<html lang="">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    {{ header_meta_tags | raw }}
    <title>{% block title %}{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="shortcut icon" type="image/x-icon" href="{{ assetUrl('zid-logo.svg') }}">
    <link rel="stylesheet" href="{{ assetUrl('style.css') }}">

    <style>
        :root {
            --primary-color: {{store.colors.primary}};
            --secondary-color: {{store.colors.primary}};
        }

        [x-cloak] { display: none !important; }

        @font-face {
            font-family: "fl-icons";
            font-display: block;
            src: url(https://baronfilou.com/app/themes/flatsome/assets/css/icons/fl-icons.eot?v=3.17.0);
            src:
                url(https://baronfilou.com/app/themes/flatsome/assets/css/icons/fl-icons.eot#iefix?v=3.17.0) format("embedded-opentype"),
                url(https://baronfilou.com/app/themes/flatsome/assets/css/icons/fl-icons.woff2?v=3.17.0) format("woff2"),
                url(https://baronfilou.com/app/themes/flatsome/assets/css/icons/fl-icons.ttf?v=3.17.0) format("truetype"),
                url(https://baronfilou.com/app/themes/flatsome/assets/css/icons/fl-icons.woff?v=3.17.0) format("woff"),
                url(https://baronfilou.com/app/themes/flatsome/assets/css/icons/fl-icons.svg?v=3.17.0#fl-icons) format("svg");
        }

        @font-face {
            font-family: 'Montserrat-Arabic';
            font-style: normal;
            font-weight: 400;
            font-display: swap;
            src: url('{{ asset_url ~ "Montserrat-Arabic-Light.otf" }}') format('opentype');
            unicode-range: U+0600-06FF;
        }

        @font-face {
            font-family: 'Montserrat-Arabic';
            font-style: normal;
            font-weight: 500;
            font-display: swap;
            src: url('{{ asset_url ~ "Montserrat-Arabic-Medium.otf" }}') format('opentype');
            unicode-range: U+0600-06FF;
        }

        @font-face {
            font-family: 'Montserrat-Arabic';
            font-style: normal;
            font-weight: 700;
            font-display: swap;
            src: url('{{ asset_url ~ "Montserrat-Arabic-SemiBold.otf" }}') format('opentype');
            unicode-range: U+0600-06FF;
        }

        @font-face {
            font-family: 'Montserrat-Arabic';
            font-style: normal;
            font-weight: 900;
            font-display: swap;
            src: url('{{ asset_url ~ "Montserrat-Arabic-Bold.otf" }}') format('opentype');
            unicode-range: U+0600-06FF;
        }

        @font-face {
            font-family: 'Montserrat-Arabic';
            font-style: normal;
            font-weight: 300;
            font-display: swap;
            src: url('{{ asset_url ~ "Montserrat-Arabic-ExtraLight.otf" }}') format('opentype');
            unicode-range: U+0600-06FF;
        }

        /* cyrillic-ext */
        @font-face {
        font-family: 'Montserrat';
        font-style: normal;
        font-weight: 400;
        font-display: swap;
